{
  "name": "nestx",
  "version": "0.2.0",
  "description": "A full stack management system base on nest",
  "keywords": [
    "mongodb",
    "nest",
    "json-schema",
    "typescript"
  ],
  "homepage": "https://github.com/vellengs/nestx-server#readme",
  "bugs": {
    "url": "https://github.com/vellengs/nestx-server/issues"
  },
  "license": "MIT",
  "author": "vellengs@qq.com",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vellengs/nestx-server.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.2",
    "@nestjs/core": "^6.0.2",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/mongoose": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.2",
    "@nestjs/platform-socket.io": "^6.0.2",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/testing": "^6.0.2",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjs/websockets": "^6.0.2",
    "bcrypt": "^3.0.4",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.3",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.8.5",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.1",
    "csurf": "^1.9.0",
    "deprecate": "^1.1.0",
    "dotenv": "^4.0.0",
    "express-rate-limit": "^3.3.2",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "helmet": "^3.15.0",
    "log4js": "^4.0.2",
    "moment": "^2.19.3",
    "mongoose": "^5.4.0",
    "mysql": "^2.15.0",
    "optional": "^0.1.4",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "swagger-ui-express": "^4.0.2",
    "typeorm": "^0.2.1",
    "webpack-node-externals": "^1.6.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.26",
    "@types/compression": "0.0.36",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/express-rate-limit": "^3.3.0",
    "@types/helmet": "0.0.43",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/jszip": "^3.1.2",
    "@types/mockjs": "^1.0.0",
    "@types/mongoose": "^5.0.11",
    "@types/node": "^6.0.100",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.4",
    "codecov": "^3.0.0",
    "codelyzer": "~4.0.1",
    "concurrently": "^3.5.1",
    "jest": "^22.4.3",
    "lint-staged": "^5.0.0",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "protractor": "~5.1.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.4",
    "ts-loader": "^3.5.0",
    "ts-node": "~3.2.0",
    "tsconfig-paths": "^3.3.1",
    "tslint": "~5.7.0",
    "tslint-sonarts": "^1.6.0",
    "typescript": "~2.6.2",
    "webpack-bundle-analyzer": "^2.9.0",
    "xlsx": "^0.11.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
