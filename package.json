{
  "name": "nestx",
  "version": "0.2.0",
  "description": "A full stack management system base on nest",
  "keywords": [
    "mongodb",
    "nest",
    "json-schema",
    "typescript"
  ],
  "homepage": "https://github.com/vellengs/nestx-server#readme",
  "bugs": {
    "url": "https://github.com/vellengs/nestx-server/issues"
  },
  "license": "MIT",
  "author": "vellengs@qq.com",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vellengs/nestx-server.git"
  },
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0-rc.1",
    "@nestjs/core": "^5.0.0-rc.1",
    "@nestjs/passport": "^1.0.6",
    "@nestjs/swagger": "^1.2.2",
    "@nestjs/testing": "^5.0.0-rc.1",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^5.0.0-rc.1",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "copy-webpack-plugin": "^4.4.1",
    "core-js": "^2.5.1",
    "dotenv": "^4.0.0",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "moment": "^2.19.3",
    "mongoose": "^5.0.17",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.0.0",
    "typeorm": "^0.2.1",
    "webpack-node-externals": "^1.6.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/jszip": "^3.1.2",
    "@types/mockjs": "^1.0.0",
    "@types/mongoose": "^5.0.11",
    "@types/node": "^6.0.100",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.4",
    "codecov": "^3.0.0",
    "codelyzer": "~4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jasmine-ts": "^0.2.1",
    "jest": "^22.4.3",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sauce-launcher": "^1.2.0",
    "lint-staged": "^5.0.0",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "protractor": "~5.1.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.4",
    "ts-loader": "^3.5.0",
    "ts-node": "~3.2.0",
    "tsconfig-paths": "^3.3.1",
    "tslint": "~5.7.0",
    "tslint-sonarts": "^1.6.0",
    "typescript": "~2.6.2",
    "webpack-bundle-analyzer": "^2.9.0",
    "xlsx": "^0.11.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
